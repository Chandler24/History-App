<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="JerniganManager_RuntimeMode" value="EMULATOR"/>
  </appSettings>
  <system.serviceModel>
    <client>
      <endpoint address="net.pipe://localhost/Jernigan/JerniganManager" binding="netNamedPipeBinding" contract="CaerusSoft.Jernigan.Contracts.IJerniganManager" />
      <endpoint address="net.pipe://localhost/Jernigan/JerniganManager_Emulator" binding="netNamedPipeBinding" contract="CaerusSoft.Jernigan.Contracts.IJerniganManager_Emulator" />
      <endpoint address="net.pipe://localhost/Jernigan/JerniganManager_RealImpl" binding="netNamedPipeBinding" contract="CaerusSoft.Jernigan.Contracts.IJerniganManager_RealImpl" />
    </client>
  </system.serviceModel>  
</configuration>